  <process id="FilterScriptProcess" isExecutable="true" name="FilterScriptProcess" processType="None">
    <startEvent id="startevent1" name="Start"/>
    <userTask activiti:assignee="kermit" id="usertask1" name="Issue order request">
      <extensionElements>
        <activiti:formProperty id="name" name="Name" type="string" variable="name"/>
        <activiti:formProperty id="orderId" name="Order Id" type="long" variable="orderId"/>
        <activiti:formProperty id="orderSummary" name="Order Summary" type="string" variable="orderSummary"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="usertask1"/>
    <scriptTask activiti:autoStoreVariables="false" activiti:exclusive="true" id="scripttask1" name="Filter order summary" scriptFormat="groovy">
      <script><![CDATA[def searchString  = 'shipping'
    def replacement = 'This is a shipping request'

    def filteredSummary = orderSummary.replaceAll(searchString, replacement)
            execution.setVariable('orderSummary', filteredSummary)]]></script>
    </scriptTask>
    <sequenceFlow id="flow2" sourceRef="usertask1" targetRef="scripttask1"/>
    <exclusiveGateway default="localDelivery" gatewayDirection="Unspecified" id="exclusivegateway1" name="Exclusive Gateway"/>
    <sequenceFlow id="flow3" sourceRef="scripttask1" targetRef="exclusivegateway1"/>
    <userTask activiti:assignee="kermit"id="usertask2" name="Send to shipping delivery">
      <extensionElements>
        <activiti:formProperty id="name" name="Name" type="string" variable="name" writable="false"/>
        <activiti:formProperty id="orderId" name="Order Id" type="long" variable="orderId" writable="false"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${orderSummary=="This is a shipping delivery"}]]></conditionExpression>
    </sequenceFlow>
    <userTask activiti:assignee="kermit" id="usertask3" name="Send to local delivery">
      <extensionElements>
        <activiti:formProperty id="name" name="Name" type="string" variable="name" writable="false"/>
        <activiti:formProperty id="orderId" name="Order Id" type="long" variable="orderId" writable="false"/>
        <activiti:formProperty id="orderSummary" name="Summary" type="string" variable="orderSummary" writable="false"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="localDelivery" sourceRef="exclusivegateway1" targetRef="usertask3"/>
    <endEvent id="endevent1" name="End"/>
    <sequenceFlow id="flow6" sourceRef="usertask3" targetRef="endevent1"/>
    <endEvent id="endevent2" name="End"/>
    <sequenceFlow id="flow7" sourceRef="usertask2" targetRef="endevent2"/>
